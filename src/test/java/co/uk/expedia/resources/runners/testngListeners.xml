<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Expedia Test" verbose="1">
    <listeners>
        <listener class-name="co.uk.expedia.listeners.Listeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="Listener Test">
        <classes>
            <class name="co.uk.expedia.testsuite.LoginTest"/>
            <class name="co.uk.expedia.testsuite.FlightTest"/>
        </classes>
    </test>
</suite>
